generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Mood {
  id        Int      @id @default(autoincrement())
  emoji     String
  comment   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
